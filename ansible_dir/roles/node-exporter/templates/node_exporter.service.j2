[Unit]
Description=Node Exporter Version {{ node_exporter_version }}
After=network-online.target
[Service]
User={{ node_exporter_user }}
Group={{ node_exporter_user }}
Type=simple
ExecStart={{ node_exporter_bin }} --collector.tcpstat --collector.systemd --collector.processes --collector.sysctl --collector.lnstat --collector.softirqs
[Install]
WantedBy=multi-user.target
